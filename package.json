{
  "name": "boudreauLabs",
  "version": "1.0.0",
  "description": "Boudreau labs website",
  "scripts": {
    "gulp": "gulp watch",
    "gulp:prod": "gulp --production",
    "11ty": "npx @11ty/eleventy --watch",
    "11ty:prod": "cross-env ELEVENTY_PROD=1 npx @11ty/eleventy",
    "clean": "gulp clean",
    "sass": "sass --watch src/assets/scss:build/assets/css",
    "sass:prod": "sass --style=compressed src/assets/scss:build/assets/css",
    "prefix": "postcss build/assets/css/bdl.styles.css --replace --use autoprefixer -b 'defaults' --no-map",
    "dev": "npm-run-all clean --parallel 11ty gulp sass",
    "build": "npm-run-all clean --sequential 11ty:prod gulp:prod sass:prod prefix"
  },
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "@mkkellogg/gaussian-splats-3d": "^0.4.5",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "scrollmagic": "^2.0.8",
    "swiper": "^11.1.9",
    "three": "^0.170.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-img": "^4.0.2",
    "@jaredwray/fumanchu": "^1.4.1",
    "autoprefixer": "^10.4.16",
    "browser-sync": "^2.29.3",
    "cross-env": "^7.0.3",
    "del": "^5.1.0",
    "gulp": "^5.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-if": "^3.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-sass-glob": "^1.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-uglify": "^3.0.2",
    "handlebars": "^4.7.8",
    "html-minifier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "postcss": "^8.4.23",
    "postcss-cli": "^11.0.0",
    "pretty": "^2.0.0",
    "sass": "^1.62.1",
    "sharp": "^0.33.4"
  }
}
